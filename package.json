{
  "name": "planning-poker-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc && cp -r src/views dist/src/views",
    "start": "node dist/src/index.js",
    "dev": "npm run build && node --inspect=0.0.0.0 dist/src/index.js",
    "test": "dotenv -e .env.test npx prisma db push --force-reset && dotenv -e .env.test jest --detectOpenHandles",
    "rebuild": "docker compose build api && docker compose rm --stop -f api && docker compose up -d api",
    "push": "docker exec api npx prisma db push",
    "migrate": "docker exec api npx prisma migrate dev",
    "studio": "DATABASE_URL=postgresql://admin:admin@localhost:6000/planning_poker?schema=public npx prisma studio",
    "seed": "ts-node-dev prisma/seed.ts"
  },
  "author": "Pavol Repisk√Ω",
