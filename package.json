    "@types/morgan": "^1.9.4",
    "@types/node": "^18.14.6",
    "@types/supertest": "^2.0.12",
    "jest": "^29.5.0",
    "nodemon": "^2.0.21",
    "prisma": "^4.11.0",
    "rimraf": "^4.3.1",
